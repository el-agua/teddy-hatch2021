${tok}`\n      }\n    }).then(user => {\n      data = user.data;\n      console.log(data);\n    }).catch(e => console.log(e));\n  } else {\n    return {\n      redirect: {\n        destination: \"/login\",\n        permanent: false\n      }\n    };\n  }\n\n  return {\n    props: {\n      userData: data,\n      token: tok\n    } // will be passed to the page component as props\n\n  };\n}\nexport default LogIn;","map":null,"metadata":{},"sourceType":"module"}